package fr.cpe;
import javax.ejb.Stateless;

import fr.cpe.IAdd;
@Stateless

public class Add implements IAdd{
	public String add(String nombres[]){
		double resultat=0;
		for(int i=0;i<nombres.length;i++)
		{
			resultat+=resultat;
		}

		return "result : " +(nombres == null ||nombres.length<=0 ? "world": resultat);
		
		}
}
